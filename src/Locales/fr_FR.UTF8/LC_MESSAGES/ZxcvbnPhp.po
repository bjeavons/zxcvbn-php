#, php-format
msgid "%d second"
msgid_plural "%d seconds"
msgstr[0] "%d seconde"
msgstr[1] "%d secondes"

#: TimeEstimator.php:89
#, php-format
msgid "%d minute"
msgid_plural "%d minutes"
msgstr[0] "%d minute"
msgstr[1] "%d minutes"

#: TimeEstimator.php:94
#, php-format
msgid "%d hour"
msgid_plural "%d hours"
msgstr[0] "%d heure"
msgstr[1] "%d heures"

#: TimeEstimator.php:99
#, php-format
msgid "%d day"
msgid_plural "%d days"
msgstr[0] "%d jour"
msgstr[1] "%d jours"

#: TimeEstimator.php:104
#, php-format
msgid "%d month"
msgid_plural "%d months"
msgstr[0] "%d mois"
msgstr[1] "%d mois"

#: TimeEstimator.php:109
#, php-format
msgid "%d year"
msgid_plural "%d years"
msgstr[0] "%d année"
msgstr[1] "%d années"

#: TimeEstimator.php:112
msgid "centuries"
msgstr "siècles"

